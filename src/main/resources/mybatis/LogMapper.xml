<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.dlj.dao.LogDao">

	<!-- 保存日志 -->
	<insert id="saveLog" parameterType="cn.dlj.entity.LogDTO">
		insert into app_req_log(id,type,request,response,uid,uname,result,memo) values
		(#{id},#{type},#{request},#{response},#{uid},#{uname},#{result},#{memo})
	</insert>

</mapper>